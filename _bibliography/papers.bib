	urldate = {2020-05-14},
	institution = {Infectious Diseases (except HIV/AIDS)},
	author = {Di Domenico, Laura and Pullano, Giulia and Sabbatini, Chiara E. and Boëlle, Pierre-Yves and Colizza, Vittoria},
	month = apr,
	year = {2020},
	doi = {10.1101/2020.04.13.20063933},
	file = {Full Text:/home/shoerl/Zotero/storage/RZ2P4MPP/Di Domenico et al. - 2020 - Expected impact of lockdown in Île-de-France and p.pdf:application/pdf},
}


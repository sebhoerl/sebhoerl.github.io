	file = {Accepted Version:/home/shoerl/Zotero/storage/3YC59VKX/Strobel and Pruckner - 2023 - OMOD An open-source tool for creating disaggregat.pdf:application/pdf;ScienceDirect Snapshot:/home/shoerl/Zotero/storage/7S6YU4JC/S0198971523000923.html:text/html},
}


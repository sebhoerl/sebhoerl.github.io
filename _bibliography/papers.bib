	author = {Kim, Youngrae and Yoon, Young},
	month = jan,
	year = {2021},
	note = {Number: 21
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {contextual matching, greedy algorithm, reinforcement learning, taxi dispatching},
	pages = {2653},
	file = {Full Text PDF:/home/shoerl/Zotero/storage/SFJDLFC8/Kim and Yoon - 2021 - Zone-Agnostic Greedy Taxi Dispatch Algorithm Based.pdf:application/pdf;Snapshot:/home/shoerl/Zotero/storage/U4DLNM8F/2653.html:text/html},

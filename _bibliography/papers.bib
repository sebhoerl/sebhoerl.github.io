	keywords = {City Logistics, stakeholder involvement, urban freight modeling},
	pages = {101--115},
	file = {Full Text:/home/shoerl/Zotero/storage/THXYDUHC/Anand et al. - 2012 - City Logistics Modeling Efforts Trends and Gaps -.pdf:application/pdf;ScienceDirect Snapshot:/home/shoerl/Zotero/storage/9IBSU537/S1877042812005617.html:text/html},
}


	note = {Publisher: Emerald Publishing Limited},
	keywords = {Last-mile delivery, B2C e-Commerce, Innovation, Parcel lockers},
	pages = {213--237},
	file = {Full Text PDF:/home/shoerl/Zotero/storage/YD9UH893/Seghezzi et al. - 2022 - Parcel lockers vs. home delivery a model to compa.pdf:application/pdf},
}


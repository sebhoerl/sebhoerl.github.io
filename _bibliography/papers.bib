	year = {2014},
	keywords = {Urban freight, European CEP sector, Parcel delivery issues},
	pages = {15--22},
	file = {ScienceDirect Snapshot:/home/shoerl/Zotero/storage/6ANMXJHY/S2210539514000340.html:text/html},
}


	file = {de Bok et al. - 2020 - Exploring the impacts of an emission based truck c.pdf:/home/shoerl/Zotero/storage/NTMDY9ZA/de Bok et al. - 2020 - Exploring the impacts of an emission based truck c.pdf:application/pdf},
}

@article{zhou_dtalite_2014,

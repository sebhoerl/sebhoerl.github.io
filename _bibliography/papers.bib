	title = {A systematic review of machine learning in logistics and supply chain management: current trends and future directions},
	volume = {28},
	issn = {1463-5771},
	shorttitle = {A systematic review of machine learning in logistics and supply chain management},
	url = {https://www.emerald.com/insight/content/doi/10.1108/bij-10-2020-0514/full/html},

	pages = {214--236},
	file = {Leng and Corman - 2020 - The role of information availability to passengers.pdf:/home/shoerl/Zotero/storage/4APAJ54A/Leng and Corman - 2020 - The role of information availability to passengers.pdf:application/pdf},
}


@article{xia_sustainable_2021,
	title = {Sustainable {Renewal} {Methods} of {Urban} {Public} {Parking} {Spaces} under the {Scenario} of {Shared} {Autonomous} {Vehicles} ({SAV}): {A} {Review} and a {Proposal}},
	volume = {13},
	issn = {2071-1050},
	shorttitle = {Sustainable {Renewal} {Methods} of {Urban} {Public} {Parking} {Spaces} under the {Scenario} of {Shared} {Autonomous} {Vehicles} ({SAV})},

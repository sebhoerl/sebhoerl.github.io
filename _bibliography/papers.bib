}

@article{preis_concept_nodate,
	title = {Concept of {Operations} for {Urban} {Air} {Mobility}: {A} {Generic} {Development} {Approach}},
	abstract = {Urban Air Mobility (UAM) is an ambitiously rising field of transportation research. Through the rediscovery of personalized air travel and novel technologies such autonomous flight and electric propulsion it is anticipated by some to bring a transport revolution in the form of flying taxis. UAM in its extent is a young field and therefore a lot of research potential can be found. To handle this new and vast system a categorization into seven fields is undertaken in this paper: air traffic management, infrastructure, markets, operators, passengers, the public, and vehicles. First results from a literature review indicate an emphasize on vehicles and a neglecting of operations. For further investigation a Python-based tool for keyword clustering and quantitative literature analysis is introduced which serves to identify and categorize research gaps systematically. The goal of this paper is to structure the system of UAM on a top layer and create a reference point for ongoing efforts. The hope of the author is that through this structure definitions of Concept of Operations for UAM become more meaningful and less redundant. A generic approach for development of Concept of Operations for UAM is presented and guidelines for a structured workflow are given. This approach contains of the formulation of the research question, identification of the relevant stakeholders and their interactions, and a description of the flow of actions and decision.},
	language = {en},
	author = {Preis, Lukas},
